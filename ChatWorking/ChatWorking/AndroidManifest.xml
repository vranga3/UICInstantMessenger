<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chatworking"
    android:versionCode="1"
    android:versionName="1.0" >
 <uses-permission android:name="android.permission.INTERNET"/>
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
   <uses-permission android:name="android.permission.CAMERA" />
    
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="com.example.chatworking.Jabber"
        >
       
        <activity
            android:name="com.example.chatworking.Introduction"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.example.chatworking.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
          <activity
            android:name="com.example.chatworking.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.chatworking.Contacts"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.CONTACTS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.chatworking.MyContacts"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.MYCONTACTS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.chatworking.Chatting"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.CHATTING" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
          <activity
            android:name="com.example.chatworking.AddFriend"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.ADDFRIEND" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.chatworking.Gridview"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.GRIDVIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.chatworking.MyProfile"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.MYPROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
          <activity
            android:name="com.example.chatworking.SettingsMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chatworking.SETTINGSMENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>